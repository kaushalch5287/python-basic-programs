def add(A,B):
    result=[ [0,0,0],[0,0,0],[0,0,0] ]
    
    result= [[A[i][j] + B[i][j]  for j in range(len(A[0]))] for i in range(len(A))]
    
    for k in result:
         
              print(k)

    return 0
A=[ [3,7, 9], [1, 2, 3], [4, 5, 10] ]

B=[ [3, 1, 1], [3, 4, 5], [7, 5, 6] ]

print("Result: ")
add(A,B)
